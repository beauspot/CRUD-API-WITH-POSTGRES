{
  "name": "crud_api_with_sequelize",
  "version": "1.0.0",
  "description": "A Simple Crud API With Postgresql as the database.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev": "nodemon server",
    "sequelize": "sequelize init",
    "sequelize-init": "npx sequelize-cli init",
    "create-Authmodel": "npx sequelize-cli model:generate --name UserAuthModel --attributes username:string,role:string,email:string,password:string",
    "create-Taskmodel": "npx sequelize-cli model:generate --name Taskmodel --attributes task:string,completed:boolean",
    "migration": "npx sequelize-cli db:migrate",
    "undo-migration": "npx sequelize-cli db:migrate:undo",
    "migrate-status": "sequelize db:migrate:status",
    "seeder": "npx sequelize-cli seed:generate --name create-fake-users",
    "start-seeder": "npx sequelize-cli db:seed:all",
    "undo-seeder": "npx sequelize-cli db:seed:undo:all",
    "db-drop": "sequelize db:drop",
    "db-create": "sequelize db:create"
  },
  "author": "Beau Iyere",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "express-validator": "^6.15.0",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "morgan": "^1.10.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.30.0",
    "xss-clean": "^0.1.1"
  }
}
